using System;

namespace uri1002
{
    class Program
    {
        public static void Main(string[] args)
        {
            double A, pi, R;

            pi = 3.14159;
            R = double.Parse(Console.ReadLine());
            A = pi * Math.Pow(R, 2);

            Console.WriteLine("A=" + A.ToString("f4"));
        }
    }
}
